{
    "$schema": "./grammar.schema.json",
    "rules": {
        "DETsing,con": {
            "description": "Determiner for singular noun starting with a consonant",
            "options": [
                "a",
                "my",
                "the",
                "this"
            ]
        },
        "DETsing,vow": {
            "description": "Determiner for singular noun starting with a vowel",
            "options": [
                "an",
                "my",
                "the",
                "this"
            ]
        },
        "DETpl": {
            "description": "Determiner for plural noun",
            "options": [
                "my",
                "the",
                "these"
            ]
        },
        "JJS": {
            "options": [
                "JJScon",
                "JJSvow"
            ]
        },
        "JJScon": {
            "description": "Adjective, superlative",
            "options": [
                "big",
                "blind",
                {
                    "abbreviation": "blu",
                    "value": "blue"
                },
                "little",
                "French",
                "red"
            ]
        },
        "JJSvow": {
            "description": "Adjective, superlative",
            "options": [
                "amateur",
                {
                    "abbreviation": "Qa",
                    "value": "aquamarine"
                }
            ]
        },
        "NN": {
            "description": "Noun, singular or mass",
            "options": [
                "NNcon",
                "NNvow"
            ]
        },
        "NNcon": {
            "description": "Noun, singular or mass, starts with a consonant",
            "options": [
                {
                    "abbreviation": "py",
                    "value": "python"
                },
                "rain"
            ]
        },
        "NNvow": {
            "description": "Noun, singular or mass, starts with a vowel",
            "options": [
                {
                    "abbreviation": "acrf",
                    "value": "aircraft"
                },
                {
                    "abbreviation": "acd",
                    "value": "acid"
                },
                "ale",
                {
                    "abbreviation": "agbr",
                    "value": "algebra"
                },
                "alien",
                "alley",
                "alumni"
            ]
        },
        "NNpl": {
            "description": "Noun, plural",
            "options": [
                "NNpl,cons",
                "NNpl,vow"
            ]
        },
        "NNpl,cons": {
            "description": "Noun, plural, starting with consonant",
            "options": [
                "cats",
                "dogs"
            ]
        },
        "NNP": {
            "description": "Proper noun, singular",
            "options": [
                {
                    "abbreviation": "Fj",
                    "value": "Mount Fuji"
                },
                "Paris"
            ]
        },
        "P": {
            "description": "Preposition",
            "options": [
                "about",
                "below",
                "toward",
                "above",
                "beneath",
                "for",
                "on",
                "under",
                "across",
                "from",
                "onto",
                "underneath",
                "after",
                "between",
                "in",
                "out",
                "until",
                "against",
                "beyond",
                "in front of",
                "outside",
                "up",
                "along",
                "but",
                "inside",
                "over",
                "upon",
                "among",
                "by",
                "in spite of",
                "past",
                "up to",
                "around",
                "concerning",
                "instead of",
                "regarding",
                "with",
                "at",
                "despite",
                "into",
                "since",
                "within",
                "because of",
                "down",
                "like",
                "through",
                "without",
                "before",
                "during",
                "near",
                "throughout",
                "behind",
                "except",
                "of",
                "to"
            ]
        },
        "RBF": {
            "description": "Adverb of frequency",
            "options": [
                "always",
                {
                    "abbreviation": "frq",
                    "value": "frequently"
                },
                "never",
                "now",
                "often",
                "sometimes"
            ]
        },
        "RBT": {
            "description": "Adverb of time",
            "options": [
                "immediately",
                {
                    "abbreviation": "lW",
                    "value": "last week"
                },
                "soon",
                "yesterday"
            ]
        },
        "THERE-ARE": {
            "options": [
                "there are"
            ]
        },
        "THERE-IS": {
            "options": [
                "there is"
            ]
        },
        "VBI": {
            "description": "Intransitive verb, base form",
            "options": [
                "flys",
                "rains"
            ]
        },
        "VBTsing": {
            "description": "Transitive verb",
            "options": [
                "eats",
                "kicks",
                "paints"
            ]
        },
        "VBTpl": {
            "description": "Transitive verb",
            "options": [
                "eat",
                "kick",
                "paint"
            ]
        }
    },
    "comment": [
        "1. CC Coordinating conjunction",
        "2. CD Cardinal number",
        "3. DT Determiner",
        "4. EX Existential there",
        "5. FW Foreign word",
        "8. JJR Adjective, comparative",
        "10. LS List item marker",
        "11. MD Modal",
        "14. NNP Proper noun, singular",
        "15. NNPS Proper noun, plural",
        "16. PDT Predeterminer",
        "17. POS Possessive ending",
        "18. PRP Personal pronoun",
        "19. PRP$ Possessive pronoun",
        "20. RB Adverb",
        "21. RBR Adverb, comparative",
        "22. RBS Adverb, superlative",
        "23. RP Particle",
        "24. SYM Symbol",
        "25. TO to",
        "26. UH Interjection",
        "27. VB Verb, base form",
        "28. VBD Verb, past tense",
        "29. VBG Verb, gerund or present participle",
        "30. VBN Verb, past participle",
        "31. VBP Verb, non-3rd person singular present",
        "32. VBZ Verb, 3rd person singular present",
        "33. WDT Wh-determiner",
        "34. WP Wh-pronoun",
        "35. WP$ Possessive wh-pronoun",
        "36. WRB Wh-adverb"
    ]
}